{
  "release": {
    "tagFormat": "v${version}",
    "ci": true,
    "debug": false,
    "branches": [
      "main",
      { 
        "name": "feature/*",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular"
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  }
}
