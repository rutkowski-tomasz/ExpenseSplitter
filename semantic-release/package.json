{
  "branches": [
    "main",
    { 
      "name": "feature/*",
      "prerelease": "alpha"
    }
  ],
  "release": {
    "tagFormat": "v${version}",
    "ci": true,
    "debug": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  }
}
