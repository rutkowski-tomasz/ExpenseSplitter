{
  "release": {
    "tagFormat": "v${version}",
    "ci": true,
    "debug": false,
    "branches": [
      "main",
      { 
        "name": "feature/*",
        "prerelease": "alpha"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github"
    ]
  }
}
