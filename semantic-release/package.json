{
  "release": {
    "tagFormat": "${version}",
    "ci": true,
    "debug": true,
    "branches": [
      "main",
      {
        "name": "feature/.+",
        "prerelease": "alpha",
        "channel": "alpha"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  }
}
